        "name": "AvgDensityAndSpeedPerLocation",
        "createdDate": "Mon Nov 10 2014",
        "epaType": "Aggregate",
        "context": "14secsLocationCompositeContext",
        "inputEvents": [
          {
            "name": "AggregatedSensorRead",
